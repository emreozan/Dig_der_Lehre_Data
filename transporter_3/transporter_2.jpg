$(document).ready(function () {

    $('section.ComposerContent > div').each(function(i){
        $(this).addClass('Row' + i);
    });

    if ( $('.anon-register').length > 0 )
    {
        $('.Row2').css('padding-top','450px');
    }

    if  ( !!navigator.userAgent.match(/Trident.*rv[ :]*11\./) )
    {
        $('#VehicleTypeList').css('display','inline-block');
    }

    $.UnoSlider.defaults = {
        tooltip: true,
        indicator: false,
        navigation: { autohide: true },
        slideshow: {
            hoverPause: true,
            continuous: true,
            timer: true,
            speed: 3,
            infinite: true,
            autostart: true
        },
        responsive: true,
        block: {
            vertical: 1,
            horizontal: 1
        },
        animation: {
            speed: 500,
            delay: 50,
            transition: 'fade'
        }
    }; 

    // Track the featured sale clicks
    $('section.ComposerContent div.Control-FeaturedSales div.Sale').click(function(){
        var t = $(this).find('.Info strong').text();
        var oc = $(this).attr('onclick');
        var m = String(oc).match(/SaleStartDate=([0-9]{2})([0-9]{2})-([0-9]{2})-([0-9]{2})/i);
        var l = ( m.length == 5 )? t + '_' + m[3] + '/' + m[2] + '/' + m[1] : t;
        Analytics.TrackEvent('Banner', 'Featured sale', l );
    });
});
</script>
